# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'MonoTalk' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
	pod 'RealmSwift'
	pod 'CSV.swift', '~> 2.1'

  # Pods for MonoTalk

  target 'MonoTalkTests' do
    inherit! :search_paths
		use_frameworks!
		pod 'RealmSwift'
    # Pods for testing
  end

  target 'MonoTalkUITests' do
    inherit! :search_paths
		use_frameworks!
		pod 'RealmSwift'
    # Pods for testing
  end

end
